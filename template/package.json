{
  "name": "{{name}}",
  "version": "0.0.0",
  "private": true,
  "main": "dist/{{name}}.js",
  "module": "dist/{{name}}.mjs",
  "types": "dist/{{name}}.d.ts",
  "eslintConfig": {
    "extends": "./node_modules/@bakerface/typescript-starter/eslint"
  },
  "nyc": {
    "extends": "@bakerface/typescript-starter/nyc"
  },
  "scripts": {
    "build": "typescript-starter build src/index.ts",
    "lint": "typescript-starter lint .",
    "test": "typescript-starter test 'src/**/*.test.*'"
  },
  "devDependencies": {
    "@bakerface/typescript-starter": "^{{version}}"
  }
}
